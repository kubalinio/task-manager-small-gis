{
  "name": "task-manager-small-gis",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "build:analyze": "cross-env ANALYZE=1 npm run build",
    "build:ci": "cross-env VITE_CI=1 vite build",
    "coverage": "vitest run --coverage",
    "dev": "vite",
    "format": "prettier --write .",
    "lint": "eslint \"./src/**/*.{ts,tsx}\" && stylelint \"./src/**/*.{css,pcss,scss}\" && echo \"lint success\"",
    "lint:fix": "eslint --fix \"./src/**/*.{ts,tsx}\" && stylelint --fix \"./src/**/*.{css,pcss,scss}\" && echo \"lint success\"",
    "prepare": "husky",
    "preview": "vite preview",
    "start": "vite",
    "test": "vitest",
    "translations": "ts-node -T --project tsconfig.json ./scripts/fetch-translations.ts",
    "typecheck": "tsc --noEmit"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,html,css}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.json": [
      "prettier --write"
    ],
    "*.{css,pcss,scss}": [
      "stylelint --fix"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "4.1.3",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-collapsible": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@tanstack/react-query": "^5.69.0",
    "@tanstack/react-router": "^1.114.27",
    "axios": "^1.8.4",
    "class-variance-authority": "^0.7.1",
    "cmdk": "^1.1.1",
    "lucide-react": "^0.484.0",
    "msw": "^2.7.3",
    "qs": "^6.14.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-error-boundary": "5.0.0",
    "react-hook-form": "^7.54.2",
    "react-intl": "^7.1.9",
    "react-moveable": "^0.56.0",
    "uuid": "^11.1.0",
    "zod": "^3.24.2",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.0",
    "@commitlint/config-conventional": "19.8.0",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.1",
    "@eslint/compat": "^1.2.7",
    "@eslint/js": "^9.23.0",
    "@ianvs/prettier-plugin-sort-imports": "4.4.1",
    "@playwright/test": "^1.51.1",
    "@swc-node/register": "~1.10.10",
    "@swc/cli": "~0.6.0",
    "@swc/core": "~1.11.13",
    "@swc/helpers": "~0.5.15",
    "@tailwindcss/vite": "^4.0.16",
    "@tanstack/react-query-devtools": "^5.69.0",
    "@tanstack/router-devtools": "1.114.27",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.2.0",
    "@testing-library/user-event": "14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "22.13.13",
    "@types/react": "19.0.12",
    "@types/react-dom": "19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.0.9",
    "@vitest/ui": "^3.0.9",
    "@welldone-software/why-did-you-render": "10.0.1",
    "autoprefixer": "10.4.21",
    "babel-jest": "^29.7.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-regexp": "^2.7.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "globals": "^16.0.0",
    "husky": "9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "~26.0.0",
    "lint-staged": "15.5.0",
    "prettier": "3.5.3",
    "prettier-plugin-packagejson": "2.5.10",
    "prettier-plugin-tailwindcss": "0.6.11",
    "stylelint": "16.16.0",
    "stylelint-config-recommended": "15.0.0",
    "tailwind-merge": "^3.0.2",
    "tailwind-variants": "^1.0.0",
    "tailwindcss": "4.0.16",
    "ts-jest": "^29.3.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0",
    "vite": "^6.2.3",
    "vite-plugin-dts": "^4.5.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.9"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
